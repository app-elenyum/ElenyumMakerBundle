{
  "name": "elenyum/maker",
  "version": "1.0",
  "type": "symfony-bundle",
  "description": "This bundle make module width controller, service and entity from json or console command.",
  "keywords": ["creator", "generated-code", "auto"],
  "license": "MIT",
  "authors": [
    {
      "name": "Elenyum",
      "homepage": "https://elenyum.ru/bundles/maker"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "doctrine/dbal": "^3",
    "doctrine/doctrine-bundle": "^2.11",
    "doctrine/orm": "^3.0",
    "nette/php-generator": "^4.1",
    "symfony/console": "^5.4|^6.0|^7.0",
    "symfony/framework-bundle": "^5.4.24|^6.0|^7.0",
    "symfony/options-resolver": "^7.0",
    "symfony/security-bundle": "^7.0",
    "symfony/serializer": "^7.0",
    "symfony/validator": "^7.0",
    "symfony/yaml": "^7.0",
    "voku/anti-xss": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "symfony/browser-kit": "^7.0",
    "symfony/css-selector": "^7.0",
    "symfony/phpunit-bridge": "^7.0"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Elenyum\\Maker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elenyum\\Maker\\Tests\\": "tests/"
    }
  }
}
